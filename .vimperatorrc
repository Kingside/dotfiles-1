"3.8.2 (created: 2014/02/13 23:52:43)

source! /Users/nakamura_mac/.vimperatorrc.local

" vim: set ft=vimperator:

noremap H <Nop>
noremap L <Nop>

" shift+jkにタブ移動を割り当て
noremap J <C-n>
noremap K <C-p>

" jkに全ページスクロールを割り当て
noremap j <Space>
noremap k <S-Space>

" hlにページのバック/フォアードを割り当て
noremap h <C-o>
noremap l <C-i>

lmap <C-j> <Tab> 
lmap <C-k> <S-Tab> 

" ---------------
" [ or ] で次のページ, 前のページと書かれたリンクに遷移する
set nextpattern=\bnext\b,\bmore\b,next,^次(の)?ページ,次へ,\b次.*,→\b,^>$,^(>>|≫)$,^(>|≫),(>|≫)$
set previouspattern=\bprev|previous\b,prev,^前(の)?ページ,前へ,\b前.*,\b←,^<$,^(<<|≪)$,^(<|≪),(<|≪)

" ---------------
" 自動補完の候補
set complete=sl

" 入力欄に自動フォーカスしない
set focuscontent

" ヒントに数字を使う
set hintchars=1234890

" ヒントのフォントサイズを大きくする
hi Hint z-index:5000; font-family:Menlo; font-size:14px; color:white; background-color:red; border-color:ButtonShadow; border-width:0px; border-style:solid; padding:0px 2px 0px 2px; position:absolute;
" ---------------
" qmarks
" 使用方法
" g o g で現在タブでgithubを開く
" g n g で新規タブでgithubを開く

" go + qmark:  current tab
" gn + qmark:  new tab
qmark g https://github.com/ShuzoN
qmark m https://mail.google.com/
qmark c https://calendar.google.com/calendar/
qmark n http://news.yahoo.co.jp/
qmark t https://trello.com/
qmark y https://www.youtube.com/ 
qmark b http://namu-r21.hatenablog.com/
qmark r https://docs.ruby-lang.org/ja/2.2.0/library/_builtin.html
" ---------------

" ブラウザ上でExモードを起動し,
" source ~/.vimperator
" と打つか
